@model Student
<div class="container">

	<h1>Student Information</h1>
	<table class="table table-bordered">
		<tr>
			<th>Field</th>
			<th>Details</th>
		</tr>
		<tr>
			<td>Studnet ID </td>
			<td>@Model.Id</td>
		</tr>
		<tr>
			<td>Student Name </td>
			<td>@Model.Name</td>
		</tr>
		<tr>
			<td>Age </td>
			<td>@Model.Age</td>
		</tr>
		<tr>
			<td>Marks</td>
			<td>@Model.Marks</td>
		</tr>
	</table>
</div>