@model List<Employee>
<div ><a asp-action="AddEmp" asp-controller="Emp">Add Employee </a></div>
<div>
	<table class="table table-borderd">
		<thead>
			<tr>
				<th>Empno </th>
				<th>Ename</th>
				<th>Job Role </th>
				<th>Salary</th>
				<th>Update</th>
				<th>Delete</th>
			</tr>
		</thead>
		 
		<tbody>
			@foreach (var item in Model)
			{
			<tr>
				<td>@item.eno</td>
				<td>@item.ename</td>
				<td>@item.job</td>
				<td>@item.sal</td>
				<td><a asp-controller="Emp" asp-action="UpdateEmp" asp-route-id="@item.eno"> Update </a></td>
				<td><a asp-controller="Emp" asp-action="DeleteEmp" asp-route-id="@item.eno" >Delete</a></td>

			</tr>
			}
		</tbody>
	</table>
</div>